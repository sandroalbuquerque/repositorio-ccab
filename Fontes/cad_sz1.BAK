User Function ValidCodSZ1(pTipo)
	Local nInicio := if(pTipo = 'E', Val(SuperGetMV('ES_CODSZ1',.F.,'555001')),1)
	Local aArea   := GetArea()

	dbSelectArea('SZ1')
	dbGoBottom()                               
	IF BOF()
		cRET := STRZERO(nInicio,LEN(Z1_COD))
	ELSE
		cRET := STRZERO(Val(Z1_COD)+1,LEN(Z1_COD))
	ENDIF
    
    RestArea( aArea )
    
Return cRET
