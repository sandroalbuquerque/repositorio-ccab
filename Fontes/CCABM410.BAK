User Function SndMail(lProc)

Local nProces := 00
Local aParSM0 := {}
Local aRetSM0 := SM0->(GetArea())
Default lProc := .F.

If lProc
	SM0->(DbSeek(cEmpAnt))
	While SM0->(!Eof()) .And. SM0->M0_CODIGO == cEmpAnt
		nProces ++
		SM0->(DbSkip())
	End
	ProcRegua(nProces)
EndIf

SM0->(DbSeek(cEmpAnt))
While SM0->(!Eof()) .And. SM0->M0_CODIGO == cEmpAnt
	If lProc
		IncProc()
	EndIf
	aParSM0 := {SM0->M0_CODIGO,SM0->M0_CODFIL}
	WFSendMail(aParSM0)
	SM0->(DbSkip())
End

RestArea(aRetSM0)

Return

