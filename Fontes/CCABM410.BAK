Static Function fVldPerg()

Local      lRet := .T.
Local lLoopPerg := .T.

//Caso cancele os parametros
//não entrar na tela de liberação
While lLoopPerg
	If !Pergunte(cPerg,.T.)
		lRet      := .F.
		lLoopPerg := .F.
		Exit
	Else
		//Valida o valor digitado no parametro mv_par34
		If mv_par29 > 100
			lLoopPerg := .T.
			ShowHelpDlg("VLDPERG",	{"O parâmetro 29 '(%)Qtde A Liberar' ","está preenchido com mais do que 100%"},5,;
			{"Favor corrigir preenchimento com "  ,"até 100%"},5)
		Else
			lLoopPerg := .F.
		EndIf
		
		//Valida a opção selecionada no parametro mv_par33
		If !lLoopPerg
			If mv_par33 == 1 .And. (mv_par46 == 1)// .or. mv_par46 == 2)
				lLoopPerg := .T.
