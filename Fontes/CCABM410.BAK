Static Function fTrataQbra(cTrataStr)

Local cRet 			:= ""
Local i    			:= 0

cTrataStr  := StrTran(cTrataStr," ","")

cRet += "'"
If Len(cTrataStr) > 3
	For i:=1 to Len(cTrataStr)
		If SubStr(cTrataStr,i,1) $ "/\;-|,:*"
			cRet  += "','"
		Else
			cRet  += SubStr(cTrataStr,i,1)
		EndIf
	Next
Else
	cRet  += AllTrim(cTrataStr)
EndIf
cRet += "'"

Return(cRet)

