Static Function fTemRevisao(cParam,cBlq,cNumSfa)

Local lRet := .F.

If cParam > "0"
	If cParam == "1"
		//Todo os pré-pedidos
		If cBlq == "0"
			//É necessario revisar antes
			lRet := .T.
		EndIf
	ElseIf cParam == "2"
		//Apenas os pré-pv originados do portal
		If cBlq == "0" .And. !Empty(cNumSfa)
			//É necessario revisar antes
			lRet := .T.
		EndIf
	ElseIf cParam == "3"
		//Apenas os pré-pv originados do microsiga
		If cBlq == "0" .And.  Empty(cNumSfa)
			//É necessario revisar antes
			lRet := .T.
		EndIf
	EndIf
EndIf

Return(lRet)


