				aCabec[aScan(aCabec,{|x|AllTrim(x[1])=="C5_NUM"    })][2],;
				aCabec[aScan(aCabec,{|x|AllTrim(x[1])=="C5_CLIENT" })][2],;
				aCabec[aScan(aCabec,{|x|AllTrim(x[1])=="C5_LOJAENT"})][2],;
