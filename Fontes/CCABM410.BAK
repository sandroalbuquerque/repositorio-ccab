If nCol == 3
	aSort(oGdDiv1:aCols,,,{|x,y| x[n1]+x[n2] < y[n1]+y[n2] })
ElseIf nCol == 4
	aSort(oGdDiv1:aCols,,,{|x,y| x[n2]+x[n1] < y[n2]+y[n1] })
