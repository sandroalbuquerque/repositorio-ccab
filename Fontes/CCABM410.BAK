Static Function fPesqAcols(aCpos,nLin)

Local y    := 0
Local nPos := 0
Local cRet := ""


For y:=1 To Len(aCpos)
	nPos := aScan(oGdTrt:aHeader,{|x|AllTrim(x[2])==AllTrim(aCpos[y])})
	If oGdTrt:aHeader[nPos][8] == "C"
		cRet += oGdTrt:aCols[nLin][nPos]
	Else
		cRet := oGdTrt:aCols[nLin][nPos]
		y := Len(aCpos)
	EndIf
Next y

Return(cRet)


