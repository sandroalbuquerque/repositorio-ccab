Local _aArea    := GetArea()
Local _aAreaSA1 := SA1->(GetArea())
Local _aAreaCTH := CTH->(GetArea())
Local _cXCLVL   := "C"+M->A1_COD+M->A1_LOJA
Local _lRet     := .T.
                       
M->A1_XCLVL     := _cXCLVL

If Inclui 
	DbSelectArea("CTH")
	DbSetOrder(1)
	If !DbSeek(xFilial("CTH")+M->A1_XCLVL)
		RecLock("CTH",.T.)
		CTH_FILIAL  := xFilial("CTH")
		CTH_CLVL    := _cXCLVL
		CTH_CLASSE  := "2"
        CTH_NORMAL  := "2"
		CTH_DESC01  := M->A1_NOME
		CTH_BLOQ    := "2"
		CTH_DTEXIS  := CTOD("01/01/1980")
		CTH_CLVLLP  := _cXCLVL
		MsUnlock()
	Else
		Alert("Item Contabil C"+M->A1_COD+M->A1_LOJA+" ja Cadastrado!")
		_lRet := .f.
	EndIf

Endif

RestArea(_aAreaSA1)
RestArea(_aAreaCTH)
RestArea(_aArea)

Return(_lRet)*/
