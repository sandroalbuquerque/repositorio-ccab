//If inclui
If  altera == .F.  .and. inclui == .F.
	
	dbSelectArea("SC5")
	dbSetOrder(1)
	dbSeek(xFilial("SC5")+ SC5->C5_NUM)
	
	cCliente:= ALLTRIM(SC5->C5_CLIENTE)
	cXclient:= ALLTRIM(SC5->C5_XCLIENT)
	nMoeda:= Str(SC5->C5_MOEDA,1,0)
	
	
	dbSelectArea("SC6")
	dbSetOrder(1)
	dbGoTop()
	dbSeek(xFilial("SC6")+ SC5->C5_NUM)
	While !EOF() .and. SC5->C5_NUM == SC6->C6_NUM
		
		// Obtém o código do produto
		
