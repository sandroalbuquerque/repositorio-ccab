User Function M410AGRV()

Local cMsg      := ""
Local cTitulo
Local lResult
Local cEmail
Local cMail1
Local cConteudo := ""
Local nOpcao    := PARAMIXB[1]
Local nPosProd  := aScan(aHeader,{|x| Upper(AllTrim(x[2])) == "C6_PRODUTO"})
Local nPosItem  := aScan(aHeader,{|x| Upper(AllTrim(x[2])) == "C6_ITEM"})
Local lItemExcl := .F.
Local cNumPrePV := ""

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Inicio da Personalizacao Por EDGAR SERRANO - Personalizacao para Pedidos Conta e Ordem          ³
If l410Auto
	Return .T.
EndIf
//³ Fim da Personalizacao Por EDGAR SERRANO - Personalizacao para Pedidos Conta e Ordem             ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Inicio da Personalizacao Por FELIPE MELO - Personalizacao para Pré-Pedidos                      ³

//Tratar alteração
If AllTrim(Str(nOpcao)) == "2" .And.  Altera .And. !Inclui
	SZA->(DbSetOrder(2))
	SZ3->(DbSetOrder(1))
	lItemExcl := .F.
	cNumPrePV := ""
	For x:=1 To Len(aCols)
		//Analisa somente itens deletados
		If aCols[x][Len(aHeader)+1]
			If SZA->(DbSeek(xFilial("SZA")+cFilAnt+M->C5_NUM+aCols[x][nPosItem]))
				//Trata status
				If SZ2->(DbSeek(xFilial("SZ2")+SZA->ZA_PRENUM))
					lItemExcl := .T.
					cNumPrePV := SZA->ZA_PRENUM
					RecLock("SZ2",.F.)
					SZ2->Z2_LIBEROK := " "
					SZ2->(MsUnLock())
				EndIf
				
				If SZ3->(DbSeek(xFilial("SZ3")+SZA->ZA_PRENUM+SZA->ZA_PREITEM))
					
					lItemExcl := .T.
					cNumPrePV := SZA->ZA_PRENUM
					//Trata itens pre-pedido
					Reclock("SZ3",.F.)
					SZ3->Z3_QTDLIB -= SZA->ZA_QTDLIBE
					SZ3->Z3_QTDPEN += SZA->ZA_QTDLIBE
					SZ3->(MsUnlock())
					
					//Trata itens movimentados
					RegToMemory("SZA")
					aStruSZA := SZA->(DbStruct())
					RecLock("SZA",.T.)
					For y:=1 To Len(aStruSZA)
						&("SZA->"+aStruSZA[y][1]) := &("M->"+aStruSZA[y][1])
					Next y
					SZA->ZA_QTDLIBE := -1*SZA->ZA_QTDLIBE
					SZA->(MsUnlock())
				EndIf
			EndIf
		EndIf
	Next x
EndIf

//Tratar exclusão
If AllTrim(Str(nOpcao)) == "3" .And. !Altera .And. !Inclui
	SZA->(DbSetOrder(2))
	SZ3->(DbSetOrder(1))
	lItemExcl := .F.
	cNumPrePV := ""
	For x:=1 To Len(aCols)
		If SZA->(DbSeek(xFilial("SZA")+cFilAnt+M->C5_NUM+aCols[x][nPosItem]))
			//Trata status
			If SZ2->(DbSeek(xFilial("SZ2")+SZA->ZA_PRENUM))
				lItemExcl := .T.
				cNumPrePV := SZA->ZA_PRENUM
				RecLock("SZ2",.F.)
				SZ2->Z2_LIBEROK := " "
				SZ2->(MsUnLock())
			EndIf
			
			If SZ3->(DbSeek(xFilial("SZ3")+SZA->ZA_PRENUM+SZA->ZA_PREITEM))
				If SZA->ZA_PRODUTO==SZ3->Z3_PRODUTO .OR. Posicione("SB1",1,xFilial("SB1")+SZ3->Z3_PRODUTO,"B1_TIPO")=="AT"
					
					lItemExcl := .T.
					cNumPrePV := SZA->ZA_PRENUM
					//Trata itens pre-pedido
					Reclock("SZ3",.F.)
					SZ3->Z3_QTDLIB -= SZA->ZA_QTDLIBE
					SZ3->Z3_QTDPEN += SZA->ZA_QTDLIBE
					SZ3->(MsUnlock())
					
					//Trata itens movimentados
					RegToMemory("SZA")
					aStruSZA := SZA->(DbStruct())
					RecLock("SZA",.T.)
					For y:=1 To Len(aStruSZA)
						&("SZA->"+aStruSZA[y][1]) := &("M->"+aStruSZA[y][1])
					Next y
					SZA->ZA_QTDLIBE := -1*SZA->ZA_QTDLIBE
					SZA->(MsUnlock())
				Endif
			EndIf
		EndIf
	Next x
EndIf

//Trata status - Verificar todos os itens
If SZA->(DbSeek(xFilial("SZA")+cFilAnt+M->C5_NUM))
	If SZ3->(DbSeek(xFilial("SZ3")+SZA->ZA_PRENUM))
		lHouveLib := .F.
		lTemPende := .F.
		lBloqTemp := .F.
		While SZ3->(!Eof()) .And.  SZ3->(Z3_FILIAL+Z3_NUM) == xFilial("SZ3")+SZA->ZA_PRENUM
			If AllTrim(SZ3->Z3_BLQ) $ "R/S"
				lHouveLib := .T.
			EndIf
			If SZ3->Z3_QTDVEN != SZ3->Z3_QTDPEN
				lHouveLib := .T.
			EndIf
			If SZ3->Z3_QTDVEN == SZ3->Z3_QTDPEN .And. SZ3->Z3_XLIBERA == "1" .And. !(AllTrim(SZ3->Z3_BLQ) $ "R/S/C")
				lTemPende := .T.
			EndIf
			If SZ3->Z3_QTDVEN != SZ3->Z3_QTDPEN .And. SZ3->Z3_XLIBERA == "1" .And. !(AllTrim(SZ3->Z3_BLQ) $ "R/S/C") .And. SZ3->Z3_QTDPEN > 0
				lTemPende := .T.
			EndIf
			If SZ3->Z3_QTDVEN == SZ3->Z3_QTDPEN .And. SZ3->Z3_XLIBERA != "1" .And. !(AllTrim(SZ3->Z3_BLQ) $ "R/S/C")
				lBloqTemp := .T.
			EndIf
			If SZ3->Z3_QTDVEN != SZ3->Z3_QTDPEN .And. SZ3->Z3_XLIBERA != "1" .And. !(AllTrim(SZ3->Z3_BLQ) $ "R/S/C") .And. SZ3->Z3_QTDPEN > 0
				lBloqTemp := .T.
			EndIf
			
			SZ3->(DbSkip())
		End
		If SZ2->(DbSeek(xFilial("SZ2")+SZA->ZA_PRENUM))
			RecLock("SZ2",.F.)
			If lHouveLib
				SZ2->Z2_BLQ     := IIf(SZ2->Z2_BLQ=="0","0","1")
				If lTemPende .Or. lBloqTemp
					SZ2->Z2_LIBEROK := " "
				Else
					SZ2->Z2_LIBEROK := "1"
				EndIf
			Else
				SZ2->Z2_LIBEROK := " "                                                                                                                       
				SZ2->Z2_BLQ     := IIf(SZ2->Z2_BLQ=="0","0"," ")
			EndIf
			SZ2->(MsUnLock())
		EndIf
	EndIf
	If lItemExcl
		U_EspelhoPV(xFilial("SZ3"),cNumPrePV,'A',,.T.)
	EndIf
EndIf

//³ Fim da Personalizacao Por FELIPE MELO - Personalizacao para Pré-Pedidos                         ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

dbSelectArea("SE4")
dbSetOrder(1)
dbGoTop()
dbSeek(xFilial("SE4")+ M->C5_CONDPAG)
If inclui
	If !Empty(SE4->E4_XEMAIL).and.!Empty(SE4->E4_XFORMUL) .and. M->C5_TIPO="N" .AND. Altera == .F.
		
		cEmail:=SE4->E4_XEMAIL
		cmail1:=SE4->E4_XMAIL1
		cConteudo:=SE4->E4_XFORMUL
		
		
		/*
		ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
		±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
		± Montando o titulo do email												±
		±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
		ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
		*/
		
		If inclui == .T.
			cTitulo := " Pedido de venda "+ALLTRIM(M->C5_NUM)+" incluido."
			cMensage := "incluido"
			cMensag1 := "Aviso de Pedido com Recebimento Antecipado"
			cMensag2 := "o qual necessita realizar pagamento antecipado para liberacao do faturamento."
			
		EndIf
		If altera == .T.
			cTitulo := "Pedido de Venda "+ALLTRIM(M->C5_NUM)+" alterado."
			cMensage := "alterado"
			cMensag1 := "Aviso de Pedido com Recebimento Antecipado"
			cMensag2 := "."
		EndIf
		If  altera == .F. .and. inclui == .F.
			cTitulo := "Pedido de Venda "+ALLTRIM(M->C5_NUM)+" excluido."
			cMensage := "excluido"
			cMensag1 := "Aviso de Exclusao de Pedido com Recebimento Antecipado"
			cMensag2 := "."
		EndIf
		
		
		//cMsg := cMsgA+CHR(13)+ CHR(10) + CHR(13) + CHR(10) +CHR(13)+ CHR(10) + CHR(13) + CHR(10) +CHR(13)+ CHR(10) + CHR(13) + CHR(10) +CHR(13)+ CHR(10) + CHR(13) + CHR(10) + cMsgB
		
		
		
		// montar assunto do email
		// -- Verifica o que esta sendo feito e coloca o respectivo conteuno na variavel a ser usada no comando "SUBJECT" do SENDMAIL
		
		// monta destinatarios do e-mail de cadastro de pedidos
		// -- Verifica condicao de pagamento e coloca os endereços na variavel a ser usada no comando "TO" do sendmail
		
		// montar mensagem do e-mail
		// -- Deve montar a mensagem do e-mail de acordo com a formula cadastrada na condição de pagamento em uma variavel para utilizar no comando "BODY" do sendmail
		/*
		ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
		±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
		± Envio do e-mail.															±
		±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
		ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
		*/    
		
		// Adicionado por Valdemir jose 06/03/2013
		_cEmail := ValidaEmail(Alltrim(cEmail)+';'+AllTrim(cmail1))
		
		
		dbSelectArea("SE4")
		dbSetOrder(1)
		dbGoTop()
		dbSeek(xFilial("SE4")+ M->C5_CONDPAG)
		
		If !Empty(SE4->E4_XEMAIL).and.!Empty(SE4->E4_XFORMUL)
			CONNECT SMTP;
			SERVER		GetMV("MV_RELSERV")	; 	// Nome do servidor de e-mail
			ACCOUNT		GetMV("MV_RELACNT")	; 	// Nome da conta a ser usada no e-mail
			PASSWORD	GetMV("MV_RELPSW") 	; 	// Senha
			RESULT   lResult             	    // Resultado da tentativa de conexão
			lResult := MailAuth(GetMV("MV_RELACNT"),GetMV("MV_RELPSW"))
			
			If lResult
				SEND MAIL 					   	 ;
				FROM 		GetMV("MV_RELACNT")	 ;   // Remetente
				TO 		    _cEmail           //Alltrim(cEmail)+';'+AllTrim(cmail1); // emails da condicao de pagamento - "Juliano.souza@agilitysolutions.com.br;julianojr_souza@hotmail.com"
				SUBJECT 	cTitulo 	 ;   // Titulo
				BODY 	  &(cConteudo); //	definir a mensagem no corpo do programa ;   // Mensagem
				RESULT		lresult					// Resultado da tentativa de conexao
			EndIf
			
			DISCONNECT SMTP SERVER
		EndIf
		
		If !lResult
			MsgAlert("Nao foi possivel enviar o email!")
		Endif
	Endif
EndIf
Return
